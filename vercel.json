{
  "version": 2,
  "public": true,
  "name": "app",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "functions": {
    "api/index.js": {
      "includeFiles": "dist/app/**",
			"excludeFiles": "node_modules/**",
			"memory": 3009,
			"maxDuration": 30
    }
  }
}
